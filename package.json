  "devDependencies": {
    "elm-hot": "^1.0.1",
    "node-elm-compiler": "^5.0.1",
    "parcel-bundler": "^1.11.0"
  },
  "dependencies": {
    "express": "^4.16.4",
    "lowdb": "^1.0.0",
    "shortid": "^2.2.14",
    "ws": "^6.1.4"
  }
